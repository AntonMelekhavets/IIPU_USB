<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ControllerOfBaseWindow">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="500.0">
         <children>
            <TableView fx:id="table" layoutX="15.0" layoutY="14.0" prefHeight="336.0" prefWidth="470.0" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="14.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="68.0" text="Метка" />
                <TableColumn fx:id="diskColumn" prefWidth="75.0" text="Том" />
                  <TableColumn fx:id="allSpaceColumn" prefWidth="75.0" text="Всего (Мб)" />
                  <TableColumn fx:id="freeSpaceColumn" prefWidth="75.0" text="Свободно (Мб)" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="ejectButton" disable="true" layoutX="15.0" layoutY="361.0" mnemonicParsing="false" text="Извлечь" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" />
            <Button fx:id="exitButton" layoutX="433.0" layoutY="361.0" mnemonicParsing="false" text="Выход" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
